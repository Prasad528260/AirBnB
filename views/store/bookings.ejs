<%- include('../partials/head') %>
  </head>
  <body>
    <%- include('../partials/nav') %>
    <main class="container mx-auto mt-20 text-center">
      <h2 class="text-4xl text-red-600 font-extrabold text-center mb-8 tracking-tight">
        My Bookings
      </h2>
      <p class="text-2xl text-gray-700 mb-8">Bookings will appear here</p>
      <% if (bookings.length === 0) { %>
        <p class="text-2xl text-gray-700 mb-8">No Bookings Found</p>
      <% } else { %>
        <div class="flex flex-wrap justify-center gap-6">
          <% bookings.forEach((booking) => { %>
            <%- include('../partials/booking', { booking: booking }) %>
          <% }) %>
        </div>
      <% } %> 
    </main>
  </body>
</html>